  "license": "MIT",
  "dependencies": {
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.16",
    "@types/node": "^12.6.9",
    "axios": "^0.19.0",
    "eslint": "^6.1.0",
    "eslint-plugin-jest": "^22.14.1",
    "ghooks": "^2.0.4",
    "jest": "^24.8.0",
    "lodash.uniq": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.5.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mendaomn/my-own-automation-server.git"
