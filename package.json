  "description": "Implementation of a simple automation server",
  "main": "src/index.js",
  "engines": {
    "node": "10.15.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "lint": "eslint src",
    "lint:types": "tsc -p ./jsconfig.json",
